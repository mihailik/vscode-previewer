{
  "name": "tty-wasm",
  "displayName": "tty-wasm",
  "description": "Terminal in WASM for VSCode",
  "version": "0.1.0",
  "publisher": "mihailik",
  "repository": "https://github.com/mihailik/vscode-previewer",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "browser": "./t.t.y.js",
  "contributes": {
      "terminal.profiles": {
        "viewType": "tty-wasm",
        "title": "Terminal in WASM",
        "icon": {
          "id": "terminal"
        }
      },
    "commands": [
      {
        "command": "tty-wasm.createTerminal",
        "title": "TTY Terminal in WASM"
      }
    ]
  },
  "scripts": {
    "start": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. .",
    "start-vtwfix": "node ../vscode-test-web --browserType=chromium --extensionDevelopmentPath=. .",
    "start-frame": "node ./t.t.y.js",
    "vsce_package": "vsce package",
    "vsce_publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "*",
    "@types/vscode": "*",
    "@vscode/test-web": "*",
    "@vscode/vsce": "*"
  }
}