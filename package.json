{
  "name": "web-previewer",
  "displayName": "web-previewer",
  "description": "",
  "version": "1.0.1",
  "publisher": "mihailik",
  "repository": "https://github.com/mihailik/vscode-previewer",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:web-previewer.activeDocumentAsHtml"
  ],
  "browser": "./index.js",
  "contributes": {
    "commands": [
      {
        "command": "web-previewer.previewDocumentAsHtml",
        "title": "üåç HTML Preview"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "web-previewer.previewDocumentAsHtml",
          "when": "resourceExtname == .html || resourceExtname == .htm"
        }
      ],
      "editor/context": [
        {
          "command": "web-previewer.previewDocumentAsHtml",
          "when": "editorLangId == html"
        }
      ],
      "editor/title": [
        {
          "command": "web-previewer.previewDocumentAsHtml",
          "when": "resourceExtname == .html || resourceExtname == .htm"
        }
      ],
      "editor/title/context": [
        {
          "command": "web-previewer.previewDocumentAsHtml",
          "when": "resourceExtname == .html || resourceExtname == .htm"
        }
      ],
      "commandPalette": [
        {
          "command": "web-previewer.previewDocumentAsHtml",
          "when": "editorLangId == html"
        }
      ]
    }
  },
  "scripts": {
    "start": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. .",
    "vsce_package": "vsce package",
    "vsce_publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@vscode/test-web": "^0.0.32",
    "vsce": "^2.14.0"
  }
}
