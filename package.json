{
  "name": "web-previewer",
  "displayName": "web-previewer",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:web-previewer.activeDocumentAsHtml"
  ],
  "browser": "./index.js",
  "contributes": {
    "commands": [
      {
        "command": "web-previewer.activeDocumentAsHtml",
        "title": "Preview as HTML"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "web-previewer.activeDocumentAsHtml",
          "when": "editorLangId == html && isWeb"
        }
      ]
    }
  },
  "scripts": {
    "start": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. .",
    "vsce_package": "vsce package",
    "vsce_publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@vscode/test-web": "^0.0.32",
    "vsce": "^2.14.0"
  }
}
